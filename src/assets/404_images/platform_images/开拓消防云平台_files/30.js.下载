      if (value.length < 6) {
        callback(new Error('密码不能小于6位'));
      } else {
        callback();
      }
    };
    var validateIndentify = function validateIndentify(rule, value, callback) {
      if (value.length !== 4) {
        callback(new Error('请输入正确的验证码'));
      } else {
        callback();
      }
    };
