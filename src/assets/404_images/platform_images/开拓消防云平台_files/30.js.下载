    /**
     * @Description: 密码输入框明文暗文转换
     * @Date: 2019/5/5
     **/
    showPwd: function showPwd() {
      if (this.pwdType === 'password') {
        this.pwdType = '';
      } else {
        this.pwdType = 'password';
      }
    },

    // 登录操作
    /**
     * @Description: 登录操作
     * @Date: 2019/5/5
